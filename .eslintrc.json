{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended", "prettier"],
  "plugins": ["@typescript-eslint"],
  "env": { "node": true, "es6": true },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}
